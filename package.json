{
  "name": "todo-tject",
  "scripts": {
    "build": "tsc --build --verbose",
    "dev": "ENV=dev tsx watch src/index.ts",
    "prestart": "bun run clean && bun run build",
    "start": "ENV=prod bun run dist/index.js",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@kanian77/tject": "^1.2.0",
    "hono": "^4.10.4",
    "http-status-codes": "^2.3.0"
  },
  "devDependencies": {
    "@hono/node-server": "^1.19.5",
    "@types/bun": "latest",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
